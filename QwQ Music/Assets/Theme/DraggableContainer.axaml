<ResourceDictionary
    xmlns="https://github.com/avaloniaui"
    xmlns:customControls="clr-namespace:QwQ_Music.Controls"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--  Add Resources Here  -->

    <ControlTheme TargetType="customControls:DraggableContainer" x:Key="{x:Type customControls:DraggableContainer}">
        <Setter Property="Template">
            <ControlTemplate>
                <Border
                    Background="{TemplateBinding Background}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    CornerRadius="{TemplateBinding CornerRadius}"
                    Name="PART_Border">
                    <Canvas Name="PART_InnerCanvas" />
                </Border>
            </ControlTemplate>
        </Setter>
    </ControlTheme>

</ResourceDictionary>
